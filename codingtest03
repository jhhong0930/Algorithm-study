폰캣몬

import java.util.*;

class Solution {
    public int solution(int[] nums) {
                 
        ArrayList<Integer> poket = new ArrayList<Integer>();
        int max= nums.length/2;       
        Arrays.sort(nums);     
        
        for(int i=0; i<nums.length; i++){
            
            if(!poket.contains(nums[i])){                
                poket.add(nums[i]);
                if(poket.size()==max){
                    break;
                } 
                
            }
        }
       int answer = poket.size();
        return answer;
    }
}
